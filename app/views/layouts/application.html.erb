<!DOCTYPE html>
<html>
<head>
  <title>Practiceproblemportal</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= nav_bar fixed: :top do %>
      <%= menu_group do %>
        <%= menu_item "CS61A Pratice Portal" %>
        <%= menu_item "Problems", problems_path %>
      <% end %>
      <%= menu_group pull: :right do %>
        <%= menu_item "Sign Up", new_student_path %>
        <%= drop_down "Login" do %>
          <%= button_to "Instructor Login", login_instructors_path, class: "btn btn-primary btn-block " %>
          <%= button_to "Student Login", login_students_path, class: "btn btn-success btn-block" %>
        <% end %>
      <% end %>
    <% end %>
    <%= yield %>

</body>
</html>
